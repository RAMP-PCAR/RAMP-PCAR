{
    "default_basemap":	"<a href='javascript:void(0);'>
                            <div class='esriBasemapGalleryLabelContainer'>
                                <span alt='{%= o.data.name %} ({%= o.data.type %})' title='{%= o.data.name %} ({%= o.data.type %})'>{%= o.data.name %} ({%= o.data.type %})</span>
                                <span class='font-xsmall extra'>{%= o.data.description %}</span>
                            </div>
                            <img class='esriBasemapGalleryThumbnail' src='{%= o.data.thumbnail %}' alt='{%= o.data.altText %}'>
                            <div class='clear'></div>
                        </a>",

    "base_tool_float":  "<div class='advanced-output-float'>                            
                            <div class='float-content'></div>
                            <button class='float-default-button button button-info'>{%= o.clearMapButton %}</button>
                        </div>",

    "population_output":  "<div class='population-info'>
                            <b>{%= o.totalPopulationLabel %}:</b>
                            <br />
                            <span class='population-output'>{%= o.populationOutput %}</span>
                        </div>",

    "buffer_output": "<div class='buffer-info'>
                        <b>{%= o.distanceLabel %}:</b><br />
                        <input type='text' class='distance-input' size='5' value='40'> km
                    </div>",

    "measure_output":  "<div class='measurement-info'>
                            <b>{%= o.areaLabel %}:</b>
                            <span class='area-output'>{%= o.areaOutput %}</span>
                            <br />
                            <b>{%= o.lengthLabel%}:</b>
                            <span class='length-output'>{%= o.lengthOutput %}</span>
                        </div>"   
}

